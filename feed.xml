  &lt;table width="100%" cellpadding="0" cellspacing="0" border="0" style="background-color: #FFFFFF; border: 1px solid #E8E6E1; border-radius: 12px; margin-bottom: 8px;"&gt;
  &lt;tr&gt;
  &lt;td style="padding: 20px 24px;"&gt;
    &lt;table width="100%" cellpadding="0" cellspacing="0" border="0" style="margin-bottom: 12px;"&gt;
